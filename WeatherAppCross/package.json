{
  "name": "WeatherApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:ios": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "start:macos": "node node_modules/react-native-macos/local-cli/cli.js start --reset-cache",
    "ios": "react-native run-ios --simulator=\"iPhone 6\"",
    "ios-device": "react-native run-ios --device \"🍑💨\"",
    "ios-device-release": "react-native run-ios --configuration Release --device \"🍑💨\"",
    "macos": "react-native run-macos",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.6.1",
    "react-native": "0.57.5",
    "react-native-macos": "^0.18.1"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-native": "^4.0.1",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "^0.49.2",
    "react-test-renderer": "16.6.1"
  }
}
